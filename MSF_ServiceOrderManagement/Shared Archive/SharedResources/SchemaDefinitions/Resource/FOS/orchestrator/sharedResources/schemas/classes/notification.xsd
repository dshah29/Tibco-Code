	<xs:element name="planItemNotification">
		<xs:annotation>
			<xs:documentation>Notification for a change in plan item status.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="planItemID" type="xs:string">
					<xs:annotation>
