				<xs:element ref="ord:order" minOccurs="0"/>
				<xs:element ref="plan:plan" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="businessTransactionID" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="orderRejectNotification">
