<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://schemas.xmlsoap.org/wsdl/" xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/" xmlns:tns2="http://v2.connect.engage.three.com" xmlns:apachesoap="http://xml.apache.org/xml-soap" xmlns:impl="urn:RegisterCustomerV2" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:wsdlsoap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:intf="urn:RegisterCustomerV2" targetNamespace="urn:RegisterCustomerV2">
    <types>
        <schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:RegisterCustomerV2">
            <import namespace="http://schemas.xmlsoap.org/soap/encoding/" schemaLocation="../../SchemaDefinitions/Standard/SOAP11/soap-encoding11.xsd"/>
            <!--schemaLocation="http://schemas.xmlsoap.org/soap/encoding/"/>-->
            <import namespace="http://v2.connect.engage.three.com"/>
            <complexType name="ArrayOf_xsd_string">
                <annotation>
                    <documentation>This is an array of catalogue item ids. It is being used in message register</documentation>
                </annotation>
                <complexContent>
                    <restriction base="soapenc:Array">
                        <attribute ref="soapenc:arrayType" wsdl:arrayType="xsd:string[]"/>
                    </restriction>
                </complexContent>
            </complexType>
            <complexType name="ArrayOf_tns2_Address">
                <annotation>
                    <documentation>This is an array of complex type Address(es). It is being used in message matchAddress</documentation>
                </annotation>
                <complexContent>
                    <restriction base="soapenc:Array">
                        <attribute ref="soapenc:arrayType" wsdl:arrayType="tns2:UKAddress[]"/>
                    </restriction>
                </complexContent>
            </complexType>
            <complexType name="ArrayOf_tns2_ReasonText">
                <annotation>
                    <documentation>This is an array of complex type Reason text(s). It is being used in message credit check</documentation>
                </annotation>
                <complexContent>
                    <restriction base="soapenc:Array">
                        <attribute ref="soapenc:arrayType" wsdl:arrayType="xsd:string[]"/>
                    </restriction>
                </complexContent>
            </complexType>
            <complexType name="ArrayOf_tns2_flag">
                <annotation>
                    <documentation>This is an array of complex type flags. It is being used in message Insurance and advocate</documentation>
                </annotation>
                <complexContent>
                    <restriction base="soapenc:Array">
                        <attribute ref="soapenc:arrayType" wsdl:arrayType="tns2:flag[]"/>
                    </restriction>
                </complexContent>
            </complexType>
            <complexType name="ArrayOf_tns2_addon">
                <annotation>
                    <documentation>This is an array of complex type addon</documentation>
                </annotation>
                <complexContent>
                    <restriction base="soapenc:Array">
                        <attribute ref="soapenc:arrayType" wsdl:arrayType="tns2:addon[]"/>
                    </restriction>
                </complexContent>
            </complexType>
            <complexType name="ArrayOf_tns2_AdditionalCharge">
                <annotation>
                    <documentation>This is an array of complex type additional charge</documentation>
                </annotation>
                <complexContent>
                    <restriction base="soapenc:Array">
                        <attribute ref="soapenc:arrayType" wsdl:arrayType="tns2:AdditionalCharge[]"/>
                    </restriction>
                </complexContent>
            </complexType>
        </schema>
        <schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://v2.connect.engage.three.com">
            <import namespace="http://schemas.xmlsoap.org/soap/encoding/" schemaLocation="../../SchemaDefinitions/Standard/SOAP11/soap-encoding11.xsd"/>
            <!--schemaLocation="http://schemas.xmlsoap.org/soap/encoding/"/>-->
            <import namespace="urn:RegisterCustomerV2"/>
            <complexType name="Order">
                <annotation>
                    <documentation>Order holds the catalogue item ids for creating the portfolio held by customer.</documentation>
                </annotation>
                <sequence>
                    <element maxOccurs="1" minOccurs="0" name="catalogue" type="xsd:string">
                        <annotation>
                            <documentation>Catalogue name.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="1" name="proposition" type="xsd:string">
                        <annotation>
                            <documentation>Unique id for package name or an offer such as 'Video Talk' price plans.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="1" name="tariff" type="xsd:string">
                        <annotation>
                            <documentation>Unique id for tariff plan such as 'Video Talk 500' price plans.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="1" name="device" type="xsd:string">
                        <annotation>
                            <documentation>Unique id for handset such as 'NEC e616'.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="0" name="overridePrice" type="xsd:double">
                        <annotation>
                            <documentation>Price to override the Sprint Catalogue Price.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="0" name="overrideDeviceChargeCode" type="xsd:string">
                        <annotation>
                            <documentation>Overridden Charge Code against the Sprint One to One mapping (Tariff to Handset) </documentation>
                        </annotation>
                    </element>
                    <element name="additions" nillable="true" type="impl:ArrayOf_xsd_string">
                        <annotation>
                            <documentation>Array of unique id(s) for catologue items such as add ons.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="0" name="expectedDeliveryDate" type="xsd:string">
                        <annotation>
                            <documentation>Shipping Date for preorder sale.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </complexType>
            <simpleType name="SaleType">
                <annotation>
                    <documentation>It is an enumeration for identifying the sale type. i.e., prepay or postpay.</documentation>
                </annotation>
                <restriction base="xsd:string">
                    <enumeration value="PayMonthly"/>
                    <enumeration value="ThreePay"/>
                </restriction>
            </simpleType>
            <simpleType name="CancellationReasonCode">
                <annotation>
                    <documentation>It is an enumeration for identifying the Cancellation Reson Code.(Credit Decline,Customer no longer requires handset,Duplicate Order, Incorrect details entered,3rd party retailer/ other decision</documentation>
                </annotation>
                <restriction base="xsd:string">
                    <enumeration value="DECLD"/>
                    <enumeration value="NOTRQ"/>
                    <enumeration value="DUPL"/>
                    <enumeration value="INCRT"/>
                    <enumeration value="THIRD"/>
                </restriction>
            </simpleType>
            <complexType name="AddressMatch">
                <annotation>
                    <documentation>It holds the attributes required for matching the customer address. At least, one other attribute apart from postal code and surname is mandatory.</documentation>
                </annotation>
                <sequence>
                    <element maxOccurs="1" minOccurs="1" name="postcode" type="xsd:string">
                        <annotation>
                            <documentation>Postal code for the address to be matched.</documentation>
                        </annotation>
                    </element>
                    <element name="houseName" nillable="true" type="xsd:string">
                        <annotation>
                            <documentation>House name for the address to be matched.</documentation>
                        </annotation>
                    </element>
                    <element name="flatNumber" nillable="true" type="xsd:string">
                        <annotation>
                            <documentation>Flat number for the address to be matched.</documentation>
                        </annotation>
                    </element>
                    <element name="houseNumber" nillable="true" type="xsd:string">
                        <annotation>
                            <documentation>House number for the address to be matched.</documentation>
                        </annotation>
                    </element>
                    <element name="streetName" nillable="true" type="xsd:string">
                        <annotation>
                            <documentation>Street Name for the address to be matched.</documentation>
                        </annotation>
                    </element>
                    <element name="town" nillable="true" type="xsd:string">
                        <annotation>
                            <documentation>City for the address to be matched.</documentation>
                        </annotation>
                    </element>
                    <element name="locality" nillable="true" type="xsd:string">
                        <annotation>
                            <documentation>Locality for the address to be matched.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </complexType>
            <complexType name="UKAddress">
                <annotation>
                    <documentation>UK ADDRESS.</documentation>
                </annotation>
                <complexContent>
                    <extension base="tns2:BillableAddress">
                        <sequence>
                            <element name="streetName" nillable="true" type="xsd:string">
                                <annotation>
                                    <documentation>Street name of address.</documentation>
                                </annotation>
                            </element>
                            <element name="houseName" nillable="true" type="xsd:string">
                                <annotation>
                                    <documentation>House name of address.</documentation>
                                </annotation>
                            </element>
                            <element name="postcode" nillable="true" type="xsd:string">
                                <annotation>
                                    <documentation>Postal code of address.</documentation>
                                </annotation>
                            </element>
                            <element name="county" nillable="true" type="xsd:string">
                                <annotation>
                                    <documentation>County of address.</documentation>
                                </annotation>
                            </element>
                            <element name="houseNumber" nillable="true" type="xsd:string">
                                <annotation>
                                    <documentation>House number of address.</documentation>
                                </annotation>
                            </element>
                            <element name="flatNumber" nillable="true" type="xsd:string">
                                <annotation>
                                    <documentation>Flat number of address.</documentation>
                                </annotation>
                            </element>
                            <element name="town" nillable="true" type="xsd:string">
                                <annotation>
                                    <documentation>Town of address.</documentation>
                                </annotation>
                            </element>
                            <element name="locality" nillable="true" type="xsd:string">
                                <annotation>
                                    <documentation>Locality of address.</documentation>
                                </annotation>
                            </element>
                            <element name="Communal" nillable="true" type="xsd:boolean">
                                <annotation>
                                    <documentation>Communal Flag of address.</documentation>
                                </annotation>
                            </element>
                            <element name="AddressId" nillable="true" type="xsd:string">
                                <annotation>
                                    <documentation>Address ID for the returned address from match address.</documentation>
                                </annotation>
                            </element>
                        </sequence>
                    </extension>
                </complexContent>
            </complexType>
            <complexType name="BFPOAddress">
                <annotation>
                    <documentation>BFPO ADDRESS.</documentation>
                </annotation>
                <complexContent>
                    <extension base="tns2:BillableAddress">
                        <sequence>
                            <element name="serialNumberRankName" nillable="true" type="xsd:string">
                                <annotation>
                                    <documentation>Serial Number Rank Name of the address.</documentation>
                                </annotation>
                            </element>
                            <element name="companyName" nillable="true" type="xsd:string">
                                <annotation>
                                    <documentation>Company bame of address.</documentation>
                                </annotation>
                            </element>
                            <element name="battalion" nillable="true" type="xsd:string">
                                <annotation>
                                    <documentation>Batallion name of address.</documentation>
                                </annotation>
                            </element>
                            <element name="regiment" nillable="true" type="xsd:string">
                                <annotation>
                                    <documentation>Regiment name of address.</documentation>
                                </annotation>
                            </element>
                            <element name="bfpoId" nillable="true" type="xsd:string">
                                <annotation>
                                    <documentation>BFPO ID of address.</documentation>
                                </annotation>
                            </element>
                        </sequence>
                    </extension>
                </complexContent>
            </complexType>
            <complexType name="AddressHistory">
                <annotation>
                    <documentation>AddressHistory contains the information about address(es). It is being used in message createCustomer</documentation>
                </annotation>
                <sequence>
                    <element maxOccurs="1" minOccurs="1" name="currentAddress" type="tns2:BillableAddress">
                        <annotation>
                            <documentation>Current Address.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="0" name="previousAddress" type="tns2:AbstractAddress">
                        <annotation>
                            <documentation>Previous Address.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="0" name="previousPreviousAddress" type="tns2:AbstractAddress">
                        <annotation>
                            <documentation>Previous-Previous Address.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </complexType>
            <complexType name="OverseasAddress">
                <annotation>
                    <documentation>Overseas ADDRESS.</documentation>
                </annotation>
                <complexContent>
                    <extension base="tns2:AbstractAddress"/>
                </complexContent>
            </complexType>
            <simpleType name="EmploymentType">
                <annotation>
                    <documentation>It is an enumeration for identifying the employment type of customer.</documentation>
                </annotation>
                <restriction base="xsd:string">
                    <enumeration value="Not-Specified"/>
                    <enumeration value="Management"/>
                    <enumeration value="Professional"/>
                    <enumeration value="Supervisor"/>
                    <enumeration value="Skilled"/>
                    <enumeration value="SemiSkilled"/>
                    <enumeration value="Unskilled"/>
                    <enumeration value="Junior"/>
                    <enumeration value="Other"/>
                    <enumeration value="Not-Asked"/>
                </restriction>
            </simpleType>
            <simpleType name="ResidentialStatus">
                <annotation>
                    <documentation>It is an enumeration for identifying the residential status of customer. eg. owning, renting etc.</documentation>
                </annotation>
                <restriction base="xsd:string">
                    <enumeration value="Not-Specified"/>
                    <enumeration value="Owner"/>
                    <enumeration value="Council"/>
                    <enumeration value="Unfurnished"/>
                    <enumeration value="Parents"/>
                    <enumeration value="Furnished"/>
                    <enumeration value="OtherTenant"/>
                    <enumeration value="JointOwner"/>
                    <enumeration value="Other"/>
                    <enumeration value="Not-Asked"/>
                </restriction>
            </simpleType>
            <simpleType name="Title">
                <annotation>
                    <documentation>It is an enumeration for identifying the title of customer.</documentation>
                </annotation>
                <restriction base="xsd:string">
                    <enumeration value="Baroness"/>
                    <enumeration value="Baronet"/>
                    <enumeration value="Captain"/>
                    <enumeration value="Colonel"/>
                    <enumeration value="Commander"/>
                    <enumeration value="Count"/>
                    <enumeration value="Countess"/>
                    <enumeration value="Dr"/>
                    <enumeration value="Duke"/>
                    <enumeration value="Earl"/>
                    <enumeration value="Inspector"/>
                    <enumeration value="Lady"/>
                    <enumeration value="Lieutenant"/>
                    <enumeration value="Lord"/>
                    <enumeration value="Lt-Colonel"/>
                    <enumeration value="Major"/>
                    <enumeration value="Marquess"/>
                    <enumeration value="Mr"/>
                    <enumeration value="Mrs"/>
                    <enumeration value="Ms"/>
                    <enumeration value="Miss"/>
                    <enumeration value="Prince"/>
                    <enumeration value="Princess"/>
                    <enumeration value="Professor"/>
                    <enumeration value="Reverend"/>
                    <enumeration value="Reverend-d"/>
                    <enumeration value="Rt-Hon"/>
                    <enumeration value="Rt-Hon-Sir"/>
                    <enumeration value="Sheik"/>
                    <enumeration value="Sir"/>
                    <enumeration value="Viscount"/>
                </restriction>
            </simpleType>
            <simpleType name="Gender">
                <annotation>
                    <documentation>It is an enumeration for identifying the gender of customer.</documentation>
                </annotation>
                <restriction base="xsd:string">
                    <enumeration value="Male"/>
                    <enumeration value="Female"/>
                </restriction>
            </simpleType>
            <simpleType name="NetworkType">
                <annotation>
                    <documentation>It is an enumeration containing network type.</documentation>
                </annotation>
                <restriction base="xsd:string">
                    <enumeration value="Vodafone"/>
                    <enumeration value="O2"/>
                    <enumeration value="Orange"/>
                    <enumeration value="T-Mobile"/>
                    <enumeration value="Virgin"/>
                    <enumeration value="Other"/>
                </restriction>
            </simpleType>
            <simpleType name="AddressType">
                <annotation>
                    <documentation>It is an enumeration containing address type of address.</documentation>
                </annotation>
                <restriction base="xsd:string">
                    <enumeration value="Home"/>
                    <enumeration value="Work"/>
                </restriction>
            </simpleType>
            <simpleType name="TariffType">
                <annotation>
                    <documentation>It is an enumeration containing tariff type.</documentation>
                </annotation>
                <restriction base="xsd:string">
                    <enumeration value="contract"/>
                    <enumeration value="payAsYouGo"/>
                </restriction>
            </simpleType>
            <complexType name="EmailAddress">
                <annotation>
                    <documentation>It holds the email address of the customer.</documentation>
                </annotation>
                <sequence>
                    <element maxOccurs="1" minOccurs="0" name="address" type="xsd:string">
                        <annotation>
                            <documentation>address for the email address.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="0" name="addressType" type="tns2:AddressType">
                        <annotation>
                            <documentation>address type of email address defined by enumeration.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </complexType>
            <complexType name="Telephone">
                <annotation>
                    <documentation>Telephone holds the telephone details.</documentation>
                </annotation>
                <sequence>
                    <element maxOccurs="1" minOccurs="1" name="areaCode" type="xsd:string">
                        <annotation>
                            <documentation>area code of the telphone number.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="1" name="number" type="xsd:string">
                        <annotation>
                            <documentation>telephone number excluding area code.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </complexType>
            <complexType name="MobileContactNumber">
                <annotation>
                    <documentation>MobileContactNumber holds customer's existing contact number details.</documentation>
                </annotation>
                <sequence>
                    <element maxOccurs="1" minOccurs="0" name="msisdn" type="xsd:string">
                        <annotation>
                            <documentation>customer's existing contact number</documentation>
                        </annotation>
                    </element>
                </sequence>
            </complexType>
            <simpleType name="PaymentType">
                <annotation>
                    <documentation>It is an enumeration containing payment type by customer.</documentation>
                </annotation>
                <restriction base="xsd:string">
                    <enumeration value="DirectDebit"/>
                    <enumeration value="BillMe"/>
                </restriction>
            </simpleType>
            <complexType name="BankDetails">
                <annotation>
                    <documentation>BankDetails the bank details of the customer.</documentation>
                </annotation>
                <sequence>
                    <element maxOccurs="1" minOccurs="1" name="accountHolder" type="xsd:string">
                        <annotation>
                            <documentation>name of the account holder.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="1" name="accountNumber" type="xsd:string">
                        <annotation>
                            <documentation>bank account number of customer.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="1" name="sortCode" type="xsd:string">
                        <annotation>
                            <documentation>bank account sort code of customer.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </complexType>
            <complexType name="Handset">
                <annotation>
                    <documentation>Handset holds the handset details given to the customer, you need to provide one of the icccid or msisdn.</documentation>
                </annotation>
                <sequence>
                    <element name="iccid" nillable="true" type="xsd:string">
                        <annotation>
                            <documentation>iccid of the handset.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="1" name="imei" type="xsd:string">
                        <annotation>
                            <documentation>imei number of sim card.</documentation>
                        </annotation>
                    </element>
                    <element name="msisdn" nillable="true" type="xsd:string">
                        <annotation>
                            <documentation>phone number with sim card.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </complexType>
            <simpleType name="CardType">
                <annotation>
                    <documentation>It is an enumeration containing card type of Credit/Debit card given by customer for payment.</documentation>
                </annotation>
                <restriction base="xsd:string">
                    <enumeration value="Visa"/>
                    <enumeration value="Switch"/>
                    <enumeration value="Amex"/>
                    <enumeration value="JCB"/>
                    <enumeration value="Mastercard"/>
                    <enumeration value="Visa Delta"/>
                </restriction>
            </simpleType>
            <complexType name="DealerDeposit">
                <annotation>
                    <documentation>It holds the attributes required for the Dealer Deposit.</documentation>
                </annotation>
                <sequence>
                    <element maxOccurs="1" minOccurs="1" name="amount" type="xsd:double">
                        <annotation>
                            <documentation>Amount required for deposit.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="1" name="reference" type="xsd:string">
                        <annotation>
                            <documentation>Reference number for dealer deposit.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </complexType>
            <complexType name="ScoreDetails">
                <annotation>
                    <documentation>It holds the attributes for the Score Details.</documentation>
                </annotation>
                <sequence>
                    <element maxOccurs="1" minOccurs="1" name="roamingAuthorised" type="xsd:boolean">
                        <annotation>
                            <documentation>Roaming Authorised.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="1" name="monthlyCreditLimit" type="xsd:double">
                        <annotation>
                            <documentation>Monthly credit Limit.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="1" name="maxHandsets" type="xsd:int">
                        <annotation>
                            <documentation>Maximum Number of Handsets.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="1" name="reasonText" type="impl:ArrayOf_tns2_ReasonText">
                        <annotation>
                            <documentation>Reason Text.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </complexType>
            <complexType name="DepositResponse">
                <annotation>
                    <documentation>Deposit credit check response.</documentation>
                </annotation>
                <complexContent>
                    <extension base="tns2:CreditCheckResponse">
                        <sequence>
                            <element name="dealerDeposit" nillable="true" type="tns2:DealerDeposit">
                                <annotation>
                                    <documentation>Credit check result for decision deposit.</documentation>
                                </annotation>
                            </element>
                        </sequence>
                    </extension>
                </complexContent>
            </complexType>
            <complexType name="AcceptResponse">
                <annotation>
                    <documentation>Accept credit check response.</documentation>
                </annotation>
                <complexContent>
                    <extension base="tns2:CreditCheckResponse">
                        <sequence>
                            <element name="scoreDetails" nillable="true" type="tns2:ScoreDetails">
                                <annotation>
                                    <documentation>Credit check result for decision deposit.</documentation>
                                </annotation>
                            </element>
                        </sequence>
                    </extension>
                </complexContent>
            </complexType>
            <complexType name="DeclineResponse">
                <annotation>
                    <documentation>Decline credit check response.</documentation>
                </annotation>
                <complexContent>
                    <extension base="tns2:CreditCheckResponse"/>
                </complexContent>
            </complexType>
            <complexType name="ReferResponse">
                <annotation>
                    <documentation>Refer credit check response.</documentation>
                </annotation>
                <complexContent>
                    <extension base="tns2:CreditCheckResponse"/>
                </complexContent>
            </complexType>
            <complexType name="ErrorResponse">
                <annotation>
                    <documentation>Error credit check response.</documentation>
                </annotation>
                <complexContent>
                    <extension base="tns2:CreditCheckResponse"/>
                </complexContent>
            </complexType>
            <complexType abstract="true" name="CreditCheckResponse">
                <annotation>
                    <documentation>It contains credit decision  identifying the result from credit score service and dealer deposit info</documentation>
                </annotation>
            </complexType>
            <complexType name="DateHolder">
                <annotation>
                    <documentation>Holder to allow nil dates to be passed from date value types</documentation>
                </annotation>
                <sequence>
                    <element name="value" type="xsd:date"/>
                </sequence>
            </complexType>
            <complexType name="IntentionToPort">
                <annotation>
                    <documentation>Allowing customer for putting intention to port their old number.</documentation>
                </annotation>
                <sequence>
                    <element maxOccurs="1" minOccurs="1" name="msisdn" type="xsd:string">
                        <annotation>
                            <documentation>Old network mobile number</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="1" name="network" type="tns2:NetworkType">
                        <annotation>
                            <documentation>Old network name</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="1" name="tariffType" type="tns2:TariffType">
                        <annotation>
                            <documentation>Old network tariff type</documentation>
                        </annotation>
                    </element>
                </sequence>
            </complexType>
            <simpleType name="MaritalStatus">
                <annotation>
                    <documentation>It is an enumeration for identifying the marital status of customer.</documentation>
                </annotation>
                <restriction base="xsd:string">
                    <enumeration value="Not-Specified"/>
                    <enumeration value="Divorced"/>
                    <enumeration value="Married"/>
                    <enumeration value="Partnered"/>
                    <enumeration value="Separated"/>
                    <enumeration value="Single"/>
                    <enumeration value="Widowed"/>
                    <enumeration value="Other"/>
                    <enumeration value="Not-Asked"/>
                </restriction>
            </simpleType>
            <simpleType name="EmploymentStatus">
                <annotation>
                    <documentation>It is an enumeration for identifying the employment status of customer.</documentation>
                </annotation>
                <restriction base="xsd:string">
                    <enumeration value="Not-Specified"/>
                    <enumeration value="FullTime"/>
                    <enumeration value="LongTerm"/>
                    <enumeration value="SelfEmpl"/>
                    <enumeration value="Student"/>
                    <enumeration value="HousePrsn"/>
                    <enumeration value="Retired"/>
                    <enumeration value="PartTime"/>
                    <enumeration value="ShortTerm"/>
                    <enumeration value="Unemployed"/>
                    <enumeration value="Other"/>
                    <enumeration value="Not-Asked"/>
                </restriction>
            </simpleType>
            <complexType name="Duration">
                <annotation>
                    <documentation>It holds the duration at particular place.</documentation>
                </annotation>
                <sequence>
                    <element name="years" type="xsd:int"/>
                    <element name="months" type="xsd:int"/>
                </sequence>
